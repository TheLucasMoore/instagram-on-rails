<div class="posts_wrapper">
  <div class="row">
      <%= render 'post', post: @post %>

        <%= link_to "Return", posts_path %>
        <% if @post.user.id == current_user.id %>
        <%= link_to "Edit Your Post", edit_post_path(@post) %>
        <% end %>
        </div>
      </div>
    </div>
</div>